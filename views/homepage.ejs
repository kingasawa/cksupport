

<div class="container-fluid">
  <div class="row content">
    <!--left side-->
    <div id="leftSidenav" class="sidenav">
      <a href="javascript:void(0)" class="closebtn close-btn">&times;</a>
      <a href="javascript:void(0)" class="sr-only closebtn open-btn">&times;</a>

      <a href="#">About</a>
      <a href="#">Services</a>
      <a href="#">Clients</a>
      <a href="#">Contact</a>
    </div>
    <!--right side-->
    <div id="mainSidenav">
      <nav class="navbar navbar-default">
        <div class="container-fluid">
          <div class="navbar-header">
            <a class="navbar-brand" href="#">WebSiteName</a>
          </div>
          <ul class="nav navbar-nav">
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#">Page 1</a></li>
            <li><a href="#">Page 2</a></li>
            <li><a href="#">Page 3</a></li>
          </ul>
        </div>
      </nav>
      <table class="table" id="guest-info-table">
        <tbody>
        <% if (allConnect) { _.each(allConnect,function(connect) { %>
          <tr class="guest<%= connect.name %>">
            <td class="guest-name">Guest<%= connect.name %></td>
            <td class="guest-ip"><%= connect.ip %></td>
            <td class="guest-title"><a href="<%= connect.url %>"><%= connect.title %></a></td>
            <td class="guest-browser">browser</td>
            <td class="guest-status"><%= connect.status %></td>
          </tr>
        <% })} %>

        <% if (allIdle) { _.each(allIdle,function(idle) { %>
        <tr class="guest<%= idle.name %>">
          <td class="guest-name">Guest<%= idle.name %></td>
          <td class="guest-ip"><%= idle.ip %></td>
          <td class="guest-title"><a href="<%= idle.url %>"><%= idle.title %></a></td>
          <td class="guest-browser">browser</td>
          <td class="guest-status"><%= idle.status %></td>
        </tr>
        <% })} %>

        </tbody>
      </table>
    </div>

</div>
</div>


